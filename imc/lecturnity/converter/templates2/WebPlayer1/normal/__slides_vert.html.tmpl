      function thumbs_activateSlide(slideNr)
      {
        if (slideNr == g_activeThumbSlide)
          return;
        
        if (g_activeThumbSlide > 0)
        { 
          thumb_s('slideThumb' + g_activeThumbSlide).backgroundColor = '%InfoPanelBgColor%';
