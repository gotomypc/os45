      function thumbs_jumpTo(slideNr)
      {
        parent.control_seekTime(parent.g_thumbEntries[slideNr-1].timeStamp);
      }

      function slides_outputThumb(thumbNr)
      {
        var index = thumbNr - 1;
        if (parent.g_layoutNavigationDocumentStructure == 0)
          document.writeln('<tr><td id="slideThumb' + thumbNr + '" class="slide"><div class="title"><a name="slideAnchor' + thumbNr + '"></a><a href="javascript:thumbs_jumpTo(' + thumbNr + ');" title="' + parent.g_thumbEntries[index].fullTitle + '">' + parent.g_thumbEntries[index].title + '</a></div><a href="javascript:thumbs_jumpTo(' + thumbNr + ');"><img src="slides/' + parent.g_thumbEntries[index].thumbUrl + '" border="0" title="' + parent.g_thumbEntries[index].fullTitle + '" alt="' + parent.g_thumbEntries[index].fullTitle + '"></a><div class="time">' + parent.msToTime(parent.g_thumbEntries[index].timeStamp) + '</div></td></tr>');
        else
